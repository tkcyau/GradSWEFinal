<%- include("partials/header") %>
<!-- Carousel Start -->
<section class="carousel">
  <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img
          src="images/carousel-images/1.jpg"
          class="d-block w-100"
          alt="..."
        />
      </div>
      <div class="carousel-item">
        <img
          src="images/carousel-images/2.jpg"
          class="d-block w-100"
          alt="..."
        />
      </div>
      <div class="carousel-item">
        <img
          src="images/carousel-images/5.jpg"
          class="d-block w-100"
          alt="..."
        />
      </div>
      <div class="carousel-item">
        <img
          src="images/carousel-images/4.jpg"
          class="d-block w-100"
          alt="..."
        />
      </div>
    </div>
    <a
      class="carousel-control-prev"
      href="#carouselExampleControls"
      role="button"
      data-slide="prev"
    >
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a
      class="carousel-control-next"
      href="#carouselExampleControls"
      role="button"
      data-slide="next"
    >
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</section>
<!-- Carousel End -->
<!-- Cards Section-->
<section class="cards-section" id="cards">
  <h2 class="section-heading text-center">
    Aspire. Advance. Achieve.
  </h2>
  <p class="text-center">University of Virginia GradSWE</p>
  <div class="row">
    <div class="cards-column col-md-6 col-lg-4">
      <div class="card mx-auto">
        <img
          class="card-img-top"
          src="images/IMG_0615.jpg"
          alt="Card image cap"
        />
        <div class="card-body">
          <h5 class="card-title">Professional</h5>
          <p class="card-text">
            We offer seminars and workshops aimed at enhancing your professional
            career.
          </p>
        </div>
      </div>
    </div>
    <div class="cards-column col-md-6 col-lg-4">
      <div class="card mx-auto">
        <img
          class="card-img-top"
          src="images/IMG_1080.jpeg"
          alt="Card image cap"
        />
        <div class="card-body">
          <h5 class="card-title">Social</h5>
          <p class="card-text">
            Participate in our social events to make connections in the field
            and make new friends.
          </p>
        </div>
      </div>
    </div>
    <div class="cards-column col-lg-4">
      <div class="card mx-auto">
        <img
          class="card-img-top"
          src="images/IMG_1587.jpg"
          alt="Card image cap"
        />
        <div class="card-body">
          <h5 class="card-title">Outreach</h5>
          <p class="card-text">
            Join us in reaching out to tomorrow's women engineers in the local
            community.
          </p>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- Cards Section End -->

<%- include("partials/footer") %>
